lpesantes@gm:~$ git config --global user.name "lpesantesm"
lpesantes@gm:~$ git config --global user.email "lpesantesm@gmail.com"
lpesantes@gm:~$ cd /var/www/html/EXTENSIONES/
lpesantes@gm:~$ git status
